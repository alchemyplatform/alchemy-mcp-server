  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.14.0",
    "@alchemy/mcp-core": "workspace:*",
    "axios": "^1.10.0",
    "dotenv": "^16.6.1",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.8.3",
    "tsx": "^4.20.3",
    "@types/node": "^20.19.4"
  }
}

